<!DOCTYPE html>
<html>
<head>
    <title>Login Audit History</title>
    <link rel="stylesheet" href="/main.css" />
</head>
<body>
    <h1>Login Audit History</h1>
    <table border="1" cellpadding="5">
        <thead>
            <tr>
                <th>ID</th>
                <th>Username</th>
                <th>Success</th>
                <th>Timestamp</th>
            </tr>
        </thead>
        <tbody>
            <% audit.forEach(entry => { %>
                <tr>
                    <td><%= entry.id %></td>
                    <td><%= entry.username %></td>
                    <td><%= entry.success ? "Yes" : "No" %></td>
                    <td><%= entry.timestamp %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>
